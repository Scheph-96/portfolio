
<div class="header">
    <div class="greeting-text">
      <h1>Looking for a services?</h1>
      <h2>Welcome Customer.</h2>
      <h3>Take a seat and choose the service that suit you.</h3>
    </div>
    <div class="coder">
      <img src="/assets/portfolio/img/coder.png">
    </div>
  </div>
  <div class="body">
    <div class="status">
      <div class="experience-year">
        <span>5+</span>
        <p>Years Experience</p>
      </div>
      <div class="completed-projects">
        <span>75</span>
        <p>Completed Projects</p>
      </div>
      <div class="happy-customers">
        <span>60</span>
        <p>Happy Customers</p>
      </div>
    </div>
    <div class="services">
      <h2>Services</h2>
      <div class="services-content">
        <% if(services) { %>
        <% for(let i = 0; i < services.length; i++) { %>
        <div class="service">
          <h3><%= services[i].title %></h3>
          <p><%= services[i].description %></p>
              <div class="order-now" service="<%= services[i].value %>">
                <p>order now</p>
                <i class='bx bx-chevron-right'></i>
              </div>
        </div>
        <% } %>
        <% } %>
      </div>
    </div>
