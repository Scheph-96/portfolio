            <p class="menu-item-title">Projects</p>
          </li>
          <li class="sidebar-menu-item" menu-item-type="customers">
            <i class='bx bxs-user'></i>
            <p class="menu-item-title">Customers</p>
          </li>
          <li class="sidebar-menu-item" menu-item-type="mailbox">
            <i class='bx bxs-envelope'></i>
            <p class="menu-item-title">Mail Box</p>
          </li>
          <li class="sidebar-menu-item" menu-item-type="income">
            <i class='bx bxs-wallet'></i>
            <p class="menu-item-title">Income</p>
          </li>
          <li class="sidebar-menu-item" menu-item-type="analytics">
            <i class='bx bx-stats'></i>
            <p class="menu-item-title">Analytics</p>
          </li>
        </ul>
      </div>
      <div class="menu-side-bar-footer">
        <div class="theme">
          <div class="theme-identifier">
            <div class="theme-icon">
              <i class='bx bxs-moon'></i>
              <i class='bx bxs-sun'></i>
            </div>
            <p class="theme-title menu-item-title">Dark Mode</p>
          </div>
          <div class="theme-switcher">
            <span class="switch-btn"></span>
          </div>
        </div>
      </div>
    </section>
    <section class="main-content">
      <div class="container"></div>
    </section>
  </main>



